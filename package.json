{
  "name": "@harvest-profit/npk",
  "version": "0.0.7",
  "description": "NPK UI Design System",
  "source": "src/index.js",
  "main": "dist/index.js",
  "types": "types/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "test": "jest --watch",
    "test:ci": "jest",
    "cover": "jest --coverage",
    "start": "storybook dev -p 8080",
    "build:docs": "storybook build -o ./build",
    "build": "rm -rf dist && NODE_ENV=production babel src -d dist --copy-files --ignore __tests__,spec.js,test.js,__snapshots__,stories.js,stories.jsx",
    "clean": "rm -rf ./dist",
    "prepublishOnly": "npm run build",
    "create-release": "npm run cover && sh ./scripts/release",
    "lint": "eslint src stories --fix",
    "prettier": "prettier --write src stories",
    "version": "node_modules/.bin/conventional-changelog -i CHANGELOG.md -o CHANGELOG.md -p angular && git add -A CHANGELOG.md"
  },
  "browserslist": [
    "defaults"
  ],
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/HarvestProfit/npk-ui.git"
  },
  "files": [
    "LICENSE",
    "README.md",
    "dist",
    "css",
    "src",
    "types",
    "!src/**/*.stories.*",
    "!dist/**/*.stories.*",
    "!src/**/*.mdx",
    "!dist/**/*.mdx",
    "!src/**/*.spec.*",
    "!dist/**/*.spec.*",
    "!.eslintrc"
  ],
  "keywords": [
    "reactstrap",
    "bootstrap",
    "react",
    "component",
    "components",
    "react-component",
    "ui"
  ],
  "contributors": [
    "Jake Humphrey <jakehumphrey18@gmail.com> (https://github.com/humphreyja)",
    "reactstrap (https://github.com/reactstrap)"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/HarvestProfit/npk-ui/issues"
  },
  "homepage": "https://github.com/HarvestProfit/npk-ui#readme",
  "dependencies": {
    "@babel/runtime": "^7.21.0",
    "@popperjs/core": "^2.6.0",
    "classnames": "^2.2.3",
    "prop-types": "^15.5.8",
    "react-popper": "^2.2.4",
    "react-transition-group": "^4.4.2"
  },
  "peerDependencies": {
    "react": ">=17.0.0",
    "react-dom": ">=17.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.4",
    "@babel/core": "^7.21.4",
    "@babel/eslint-parser": "^7",
    "@babel/plugin-proposal-export-default-from": "^7.24.7",
    "@babel/plugin-transform-runtime": "^7.25.4",
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.24.7",
    "@babel/runtime": "^7.21.0",
    "@storybook/addon-actions": "^8.3.2",
    "@storybook/addon-docs": "^8.3.2",
    "@storybook/addon-essentials": "^8.3.3",
    "@storybook/addon-links": "^8.3.2",
    "@storybook/addon-mdx-gfm": "^8.3.2",
    "@storybook/addon-storysource": "^8.3.3",
    "@storybook/addon-styling-webpack": "^1.0.0",
    "@storybook/addon-webpack5-compiler-babel": "^3.0.3",
    "@storybook/core-events": "^8.3.2",
    "@storybook/manager-api": "^8.3.2",
    "@storybook/react": "^8.3.2",
    "@storybook/react-webpack5": "^8.3.2",
    "@storybook/theming": "^8.3.2",
    "@svgr/webpack": "^8.1.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.0.1",
    "babel-eslint": "^9.0.0",
    "babel-jest": "28.1.3",
    "babel-loader": "^8.2.2",
    "bootstrap-scss": "^4.6.0",
    "concurrently": "^7.6.0",
    "css-loader": "^7.1.2",
    "eslint": "^8.17.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-storybook": "^0.8.0",
    "jest": "28.1.3",
    "prettier": "2.7.1",
    "raw-loader": "^4.0.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-test-renderer": "^18.3.1",
    "sass": "^1.79.3",
    "sass-loader": "^16.0.2",
    "storybook-css-modules": "^1.0.8",
    "storybook-dark-mode": "^4.0.2",
    "style-loader": "^4.0.0"
  }
}